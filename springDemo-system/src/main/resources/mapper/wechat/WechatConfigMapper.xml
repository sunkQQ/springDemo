<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sunk.demo.wechat.mapper.WechatConfigMapper">
    
    <resultMap type="com.sunk.demo.wechat.domain.WechatConfig" id="BaseResultMap">
        <result property="id"    column="id"    />
        <result property="appId"    column="app_id"    />
        <result property="appSecret"    column="app_secret"    />
        <result property="wxName"    column="wx_name"    />
        <result property="wxWechatCode"    column="wx_wechat_code"    />
        <result property="token"    column="token"    />
        <result property="status"    column="status"    />
        <result property="replaycontent"    column="replayContent"    />
        <result property="menus"    column="menus"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="Base_Column_List">
        id, app_id, app_secret, wx_name, wx_wechat_code, token, status, replayContent, menus, create_by, create_time,
        update_by, update_time
    </sql>

</mapper>